<?php
$result = array();
foreach ($reservations as $reservation){
  $result[] = array(
    "id" => ($reservation->getId()),
    "title" => ($reservation->getIdUserReserve()->getName()), 
    "start" => (strtotime($reservation->getHeureDebut())),
    "end" => (strtotime($reservation->getHeureFin())),
    "url" => url_for('/reservation'),
    "color" => ($reservation->getSalle()->getCouleur()),
    "allDay" => (strtotime($event->getEndDate()) - strtotime($event->getStartDate()) > 24*3600) ? true : false
  );  
}
echo json_encode($result);
?>
